<center>
  <h1 style="color:red;">Ranking Board</h1>
</center>

<table class="table table-bordered table-striped table-hover">
  <thead class="bg-primary">
    <tr>
      <th>#</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Score</th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |user|}}
    <tr>
      <td></td>
      <td>{{user.firstName}}</td>
      <td>{{user.lastName}}</td>
      <td>{{user.score}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>
